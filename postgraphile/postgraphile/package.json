{
  "name": "postgraphile",
  "version": "5.0.0-alpha.8",
  "description": "Automatic, high performance, and highly customizable GraphQL API for PostgreSQL",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "node": "./dist/cli.js"
    },
    "./presets/amber": {
      "types": "./dist/presets/amber.d.ts",
      "node": "./dist/presets/amber.js",
      "default": "./dist/presets/amber.js"
    },
    "./presets/v4": {
      "types": "./dist/presets/v4.d.ts",
      "node": "./dist/presets/v4.js",
      "default": "./dist/presets/v4.js"
    }
  },
  "bin": "./dist/cli-run.js",
  "scripts": {
    "test": "jest -i __tests__/queries && jest -i __tests__/mutations && jest -i __tests__/schema",
    "prepack": "tsc -b tsconfig.build.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphile/heart.git"
  },
  "keywords": [
    "graphile",
    "graphql",
    "engine",
    "postgraphile",
    "pg",
    "postgres",
    "postgresql",
    "graphqljs",
    "plugin",
    "engine",
    "extension",
    "graphite"
  ],
  "author": "Benjie Gillam <code@benjiegillam.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graphile/heart/issues"
  },
  "homepage": "https://graphile.org/graphile-engine/",
  "dependencies": {
    "@dataplan/pg": "workspace:*",
    "@graphile/lru": "workspace:^",
    "@types/node": "^18.15.5",
    "@types/pg": "^8.6.2",
    "debug": "^4.3.3",
    "grafast": "workspace:^",
    "grafserv": "workspace:^",
    "graphile-build": "workspace:*",
    "graphile-build-pg": "workspace:*",
    "graphile-config": "workspace:^",
    "graphql": "16.1.0-experimental-stream-defer.6",
    "iterall": "^1.3.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "pg-introspection": "workspace:^",
    "ruru": "workspace:^",
    "tslib": "^2.5.0",
    "ws": "^8.12.1"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/express": "^4",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8",
    "@types/nodemon": "1.19.1",
    "graphile": "workspace:^",
    "graphile-export": "workspace:^",
    "jest": "^28.1.3",
    "nodemon": "^2.0.15",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
