{
  "private": true,
  "name": "postgraphql-example-forum-relay",
  "version": "1.0.0",
  "description": "A PostGraphQL forum example with a Relay frontend.",
  "scripts": {
    "schema-up": "psql -f ../forum/schema.sql",
    "schema-down": "psql -f ../forum/schema-drop.sql",
    "schema": "npm run schema-down && npm run schema-up",
    "start": "babel-node ./server",
    "update-schema": "babel-node ./scripts/updateSchema.js"
  },
  "dependencies": {
    "aphrodite": "^0.5.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-relay-plugin": "^0.9.2",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "css-loader": "^0.25.0",
    "dotenv": "^2.0.0",
    "escape-html": "1.0.3",
    "fs-promise": "^0.5.0",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.1.7",
    "jwt-decode": "^2.1.0",
    "pg": "^6.1.0",
    "postgraphql": "2.0.0-beta.7",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-relay": "^0.9.2",
    "react-relay-network-layer": "^1.3.4",
    "react-router": "^2.8.1",
    "react-router-relay": "^0.13.5",
    "sanitize.css": "^4.1.0",
    "style-loader": "^0.13.1",
