{
  "private": true,
  "name": "postgraphql-example-forum-relay",
  "version": "1.0.0",
  "description": "A PostGraphQL forum example with a Relay frontend.",
  "scripts": {
    "schema-up": "psql -f ../forum/schema.sql",
    "schema-down": "psql -c 'drop schema if exists forum_example, forum_example_utils cascade'",
    "schema": "npm run schema-down && npm run schema-up",
    "start": "babel-node ./server.js",
    "update-schema": "babel-node ./scripts/updateSchema.js"
  },
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-loader": "6.2.4",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-relay-plugin": "^0.8.1",
    "express": "4.13.4",
    "fs-promise": "^0.5.0",
    "postgraphql": "^1.5.0",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-relay": "^0.8.1",
    "react-router": "^2.4.1",
    "react-router-relay": "^0.13.2",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  }
}
