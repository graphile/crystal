      "js",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/resources/jest-setup.js"
    ],
    "browser": false,
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": "/__tests__/[^.]+-test.(t|j)s$"
