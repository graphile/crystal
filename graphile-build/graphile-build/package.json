    "type": "git",
    "url": "git+https://github.com/graphile/heart.git"
  },
  "keywords": [
    "graphile",
    "graphql",
    "engine",
    "apollo",
    "graphqljs",
    "plugin",
    "build",
    "extension",
    "graphite"
  ],
  "author": "Benjie Gillam <code@benjiegillam.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graphile/heart/issues"
  },
  "homepage": "https://graphile.org/graphile-build/",
  "dependencies": {
    "@types/node": "^18.15.5",
    "@types/pluralize": "^0.0.29",
    "@types/semver": "^7.3.9",
    "chalk": "^4.1.2",
    "debug": "^4.3.3",
    "graphile-config": "workspace:^",
    "graphql": "^16.1.0-experimental-stream-defer.6",
    "lodash": "^4.17.21",
    "pluralize": "^7.0.0",
    "semver": "^7.3.5",
    "tslib": "^2.5.0"
  },
  "engines": {
    "node": ">=16"
  },
  "peerDependencies": {
    "grafast": "workspace:^",
    "graphile-config": "workspace:^",
    "graphql": "^16.1.0-experimental-stream-defer.6"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.5.1",
    "graphile-export": "workspace:^",
    "graphql": "16.1.0-experimental-stream-defer.6",
    "jest": "^28.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
