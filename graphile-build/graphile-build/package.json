{
  "name": "graphile-build",
  "version": "5.0.0-alpha.18",
  "description": "Build a GraphQL schema from plugins",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "prepack": "tsc -b && cp src/.npmignore dist/.npmignore"
